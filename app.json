{
  "expo": {
    "name": "willfind8",
    "slug": "willfind8",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/willfind8-icon.png",
    "scheme": "willfind8",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.willfind8"
    },
    "facebookAppId": "1599106077378916",
    "facebookDisplayName": "willfind8",
    "facebookScheme": "1599106077378916",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/willfind8-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.willfind8"
    },
    "splash": {
      "image": "./assets/images/willfind8-splash.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "cover"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/willfind8-splash.png",
          "imageWidth": "100%",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      "expo-asset",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "50e78cd9-eacc-48f2-9c70-e04eb0009a0a"
      }
    }
  }
}
